{
  "name": "boilerplate-backend-restfulapi",
  "version": "1.0.0",
  "description": "Boilerplate Backend RESTful API",
  "main": "src/index.js",
  "author": "Michael Linhardt",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dktunited/boilerplate-backend-restfulapi.git"
  },
  "scripts": {
    "prestart": "npm install @michaellinhardt/helpers & npm install @michaellinhardt/restfulapi",
    "start": "NODE_ENV=development npx nodemon ./src/index.js --exec babel-node",
    "script": "NODE_ENV=development babel-node",
    "build": "rm -rf dist ; npx babel src --out-dir dist --copy-files",
    "prod": "node dist/src/index.js",
    "lint": "npx eslint ./src",
    "knex": "babel-node node_modules/.bin/knex",
    "mocha": "NODE_ENV=test npx mocha --recursive"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.21.1",
    "escope": "^4.0.0",
    "eslint": "^8.10.0",
    "eslint-config-zavatta": "^6.0.3",
    "nodemon": "^2.0.15",
    "prettyjson": "^1.2.5"
  },
  "dependencies": {
    "@michaellinhardt/helpers": "file:../npm-backend-helpers",
    "@michaellinhardt/restfulapi": "file:../npm-backend-restfulapi",
    "regenerator-runtime": "^0.13.11"
  }
}
