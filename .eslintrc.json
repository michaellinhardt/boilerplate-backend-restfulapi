{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6
  },
  "globals": {
    "PUBLIC": true,
    "MS_SECOND": true,
    "MS_MINUTE": true,
    "MS_HOUR": true,
    "MS_DAY": true,
    "MS_WEEK": true,
    "MS_MONTH": true,
    "MS_YEAR": true
  },
  "extends": [
    "airbnb-base/legacy"
  ],
  "rules": {
    "no-multiple-empty-lines": ["error", { "max": 1, "maxBOF": 0, "maxEOF": 1 }],
    "semi": ["error", "never"],
    "one-var": "off",
    "strict": "off",
    "class-methods-use-this": "off",
    "padded-blocks": ["error", "always"],
    "space-before-function-paren": ["error", "always"],
    "eol-last": "error",
    "curly": "off",
    "no-tabs": "off",
    "no-undef": "warn",
    "no-unused-vars": "warn",
    "no-inline-comments": "off",
    "no-console": "off",
    "id-length": "off",
    "indent": [
      "error",
      "tab"
    ],
    "max-len": [
      "warn",
      {
        "code": 120
      }
    ],
    "no-sync": 0,
    "camelcase": 0
  }
}
